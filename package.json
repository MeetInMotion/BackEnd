    "bookshelf": "^0.10.3",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.0",
    "eslint-plugin-mocha": "^4.9.0",
    "express": "^4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.12.0",
    "morgan": "^1.8.1",
    "nconf": "^0.8.4",
    "nodemon": "^1.11.0",
    "pg": "^6.1.5",
    "pre-push": "^0.1.1",
    "require-dir": "^0.3.1",
    "vhost": "^3.0.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "mocha": "^3.3.0",
    "proj4": "^2.4.3",
    "sinon": "^2.2.0"
  }
